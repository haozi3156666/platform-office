{
    "name": "sanatorium/office",
    "type": "platform-extension",
    "description": "Excel Word CSV PDF writers and readers for Cartalyst Platform",
    "authors": [
        {
            "name": "Sanatorium",
            "email": "info@sanatorium.ninja",
            "homepage": "http://sanatorium.ninja"
        },
        {
            "name": "rozklad",
            "email": "jan.rozklad@gmail.com",
            "homepage": "http://rozklad.me"
        },
        {
            "name": "mattluk",
            "email": "matejlukas12@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cartalyst/composer-installers": "1.2.*",
        "platform/foundation": ">=2.0 <5.0",
        "league/csv": ">=8.0.0",
        "maatwebsite/excel": ">=2.1.0",
        "akeneo/spreadsheet-parser": ">=1.1.5",
        "phpoffice/phpexcel": ">=1.8.0",
        "phpoffice/phpword": ">=0.12.0",
        "patchwork/utf8": ">=1.2.1",
        "tijsverkoyen/css-to-inline-styles": ">=1.5",
        "barryvdh/laravel-dompdf": ">=0.7.0"
    },
    "autoload": {
        "classmap": [
            "database/migrations",
            "database/seeds"
        ],
        "psr-4": {
            "Sanatorium\\Office\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "cartalyst",
        "platform",
        "extension",
        "madeinsane"
    ]
}